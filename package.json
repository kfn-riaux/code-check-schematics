{
  "name": "@kfn-modern-dx/code-check-schematic",
  "version": "0.0.2",
  "description": "Angular schematic of add auto code check settings",
  "scripts": {
    "start": " schematics .:code-check",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --exclude 'src/**/*.d.ts'"
  },
  "keywords": [
    "schematics",
    "Angular",
    "tslint",
    "stylelint",
    "prettier"
  ],
  "author": "KadoTomohiro",
  "homepage": "https://github.com/kfn-riaux/code-check-schematics",
  "bugs": "https://github.com/kfn-riaux/code-check-schematics/issues",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^8.3.17",
    "@angular-devkit/schematics": "^8.3.17",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "typescript": "~3.5.3"
  },
  "devDependencies": {
    "@schematics/angular": "^8.3.18",
    "tslint": "^5.20.1"
  }
}
